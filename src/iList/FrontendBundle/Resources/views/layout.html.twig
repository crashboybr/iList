	<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>{% block title %}Maniac, Compra e Venda Apple por todo Brasil - Iphone, Ipad, Ipod, Imac entre outros.{% endblock %}</title>
<link href="/favicon.ico" rel="shortcut icon" type="image/x-icon" />

{% block javascripts %}
	{% javascripts
		'@iListFrontendBundle/Resources/public/js/jquery-1.10.2.min.js'
		
	 %}
	    <script src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock %}

{% block stylesheets %}
	{% stylesheets 
		'bundles/ilistfrontend/css/style.css'
	filter='cssrewrite' %}
	    <link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
{% endblock %}

</head>

<body>
    <div id="wrapper">
    	<div class="container">
            <div class="header">
                <div class="logo" onclick="http://www.ilist.dev">
                	<h3>
						
					</h3>
                </div>
                <div class="sell-apple">
                	<a href="{{ path('anuncio_new') }}" class="mainButton">Venda seu Apple</a>
                </div>
                <div class="breadcrumb">
                    <div class="account">
                   	<img src="{{ asset('images/sec.gif') }}" width="23" height="24" alt="security" class="iconSec"/> 
                    <a href="{{ path('account_home') }}" class="security"> {% if app.user %}Olá {{ app.user.name }} {% else %}Acesse sua conta{% endif %}</a>
                    
                    </div>
                </div>
            </div>
            <div class="content">
            	{% block body %}{% endblock %}
           		
            </div>
            <div class="footer">
            	<ul class="list">
                	<li class="item">
                        <a href="#" class="btn-footer">Iphone</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Ipad</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Ipod</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Macbook</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Imac</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Acessórios</a>
                    </li>
                </ul>
                <ul class="list">
                	<li class="item">
                        <a href="#" class="btn-footer">Venda seu Apple</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Cadastre-se</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Concectar pelo Facebook</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Login</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer">Esqueci Minha Senha</a>
                    </li>
                    <li class="item">
                        <a href="#" class="btn-footer"></a>
                    </li>
                </ul>
                <div class="facebook"></div>
            </div>
        </div>
        
    </div>
</body>
</html>
